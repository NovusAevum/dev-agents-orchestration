{
  "description": "Elite autonomous mode: Fully autonomous agents with cost optimization",
  "permissions": {
    "defaultMode": "bypassPermissions",
    "allow": [],
    "ask": [],
    "deny": [
      "Bash(sudo:*)",
      "Bash(rm -rf /:*)",
      "Bash(rm -rf ~:*)"
    ]
  },
  "agentBehavior": {
    "alwaysRecommend": false,
    "askBeforeCritical": false,
    "explainReasoning": false,
    "proactiveMode": true,
    "costOptimization": true
  },
  "enabledPlugins": {
    "commit-commands@claude-code-plugins": true
  },
  "alwaysThinkingEnabled": false,
  "modelPreferences": {
    "defaultModel": "sonnet",
    "useHaikuForSimpleTasks": true,
    "taskComplexityThreshold": "auto"
  },
  "tokenOptimization": {
    "lazyLoading": true,
    "contextPruning": true,
    "incrementalOperations": true,
    "smartSearch": true
  }
}
